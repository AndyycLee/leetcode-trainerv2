{
  "name": "leetcode-trainer",
  "displayName": "Leetcode trainer/Link Saver",
  "version": "0.0.0",
  "description": "Save links and train on leetcode problems",
  "author": "calga",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.0.2",
    "firebase": "9.15.0",
    "plasmo": "0.61.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.6.1"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.1",
    "@types/chrome": "0.0.206",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "prettier": "2.8.1",
    "typescript": "4.9.4"
  },
  "manifest": {
    "key": "$CRX_PUBLIC_KEY",
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "permissions": [
      "identity",
      "tabs",
      "activeTab"
    ],
    "oauth2": {
      "client_id": "$PLASMO_PUBLIC_FIREBASE_CLIENT_ID",
      "scopes": [
        "https://www.googleapis.com/auth/userinfo.email",
        "https://www.googleapis.com/auth/userinfo.profile",
        "https://www.googleapis.com/auth/calendar"
      ]
    }
  }
}
